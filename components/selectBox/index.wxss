.container{
  width:100%;
  height:60rpx;
  padding: 0 20rpx;
  display: flex;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
}
.item{
  display: flex;
  height:100%;
  width:33.333333%;
  flex-direction: row;
  color: #333333;
  align-items: center;
  font-size: 12px;
}
.item-1{
  justify-content: flex-start
}
.item-2{
  justify-content: center
}
.item-3{
  justify-content: flex-end
}
.item>image{
  width: 20px;
  height:20px;
}