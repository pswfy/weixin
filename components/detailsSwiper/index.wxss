.comnainer{
  width:100%;
  height:500rpx;
  position: relative;
  background: #fff;
  top:0;
  left: 0;
  margin: 0;
  padding: 0;
}
.swiper-container{
  width: 100%;
  height: 100%;
}
.slide-image{
  width: 100%;
  height: 500px;
}
.swiper-li{
  position: absolute;
  width: 100%;
  height: 40rpx;
  background: rgba(0,0,0,.1);
  left: 0;
  bottom:0;
  box-shadow:0px -3px 10px rgba(0,0,0,.1);
  display: flex;
  justify-content: start;
}
.swiper-li-item{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  width: 16%;
  line-height: 30rpx;
}
.swiper-li-item-text{
  font-size: 26rpx;
  color: #fff;
}
.swiper-li-item-icon{
  position: absolute;
  width: 40rpx;
  height: 30rpx;
  bottom: -10rpx;
}