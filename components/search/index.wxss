.search-container{
  width:94%;
  height:72rpx;
  background: #f3f2f3;
  border-radius: 40rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.search-select{
  box-sizing: border-box;
  flex: 80px 0 0;
  display: flex;
  flex-direction: row;
  align-content: center;
  color: #4a4a4a;
  height: 100%;
  line-height: 72rpx;
}
.search-select>image{
  position: relative;
  top:14rpx;
  width: 40rpx;
  height:40rpx;
}
.search{
  flex: 1;
  display: flex;
  flex-direction: row;
  align-content: center;
  color: #9b9b9b;
  font-size: 30rpx;
}
.search>image{
  position: relative;
  width: 36rpx;
  height: 36rpx;
  margin-top: 2rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
}