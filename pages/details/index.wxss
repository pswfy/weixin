page{
  margin-bottom: 80rpx;
}
.title-address-map{
  box-sizing: border-box;
  padding: 20rpx;
  height:140rpx;
  display: flex;
}
.title-address{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.map-box{
  flex: 100rpx 0 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.title{
  margin-top: 15rpx;
  padding:0 15rpx
}
.type{
  display: flex;
  flex-wrap: nowrap;
  overflow-x: scroll;
}
.type::-webkit-scrollbar {
    display: none;
}
.configure{
  padding: 10rpx 15rpx;
}